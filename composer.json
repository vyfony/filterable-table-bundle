{
    "name": "vyfony/filterable-table-bundle",
    "type": "symfony-bundle",
    "description": "Symfony VyfonyFilterableTableBundle",
    "keywords": ["filter", "table", "filterable-table", "symfony-bundle"],
    "homepage": "https://github.com/vyfony/filterable-table-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Dyshkant",
            "email": "vyshkant@gmail.com",
            "homepage": "https://vyshkant.github.io"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "doctrine/common": "^2.0",
        "doctrine/orm": "^2.0",
        "psr/simple-cache": "^1.0",
        "symfony/cache": "^4.3 || ^5.1",
        "symfony/config": "^4.3 || ^5.1",
        "symfony/dependency-injection": "^4.3 || ^5.1",
        "symfony/doctrine-bridge": "^4.3 || ^5.1",
        "symfony/form": "^4.3 || ^5.1",
        "symfony/http-foundation": "^4.3 || ^5.1",
        "symfony/http-kernel": "^4.3 || ^5.1.5",
        "symfony/options-resolver": "^4.3 || ^5.1",
        "symfony/routing": "^4.3 || ^5.1",
        "symfony/yaml": "^4.3 || ^5.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phan/phan": "^2.4",
        "phpro/grumphp": "^0.17",
        "phpunit/phpunit": "^7.3",
        "povils/phpmnd": "^2.1",
        "sensiolabs/security-checker": "^6.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Vyfony\\Bundle\\FilterableTableBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vyfony\\Bundle\\FilterableTableBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
